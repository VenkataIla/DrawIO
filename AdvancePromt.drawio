        <mxCell id="HNa0GtHYEyGsjAsGX6pw-8" value="Did you fulfil the conditions of the prompt completely&amp;nbsp;" style="rounded=0;whiteSpace=wrap;html=1;align=left;" vertex="1" parent="WIyWlLk6GJQsqaUBKTNV-1">
